:3},"flags":{"journal-links":{},"core":{"sourceId":"Item.bxg2xSSoDL9ZUkSk"},"cf":{"id":"temp_r9d6jaay8q","path":"Savage Eberron Equipment#/CF_SEP/2.1. Meleee Weapons#/CF_SEP/Martial Weapons","color":"#000000"}}},{"_id":"8PuRkzSOYU2CMcsX","name":"War Pick","type":"weapon","img":"icons/weapons/axes/pickaxe-iron-green.webp","data":{"description":"","notes":"","additionalStats":{},"quantity":1,"weight":3,"price":10,"equippable":false,"equipped":true,"isVehicular":false,"mods":1,"actions":{"skill":"Fighting","skillMod":"","dmgMod":"","additional":{}},"damage":"@str+d6","range":"","rof":"1","ap":"2","minStr":"d6","shots":"0","currentShots":"0","ammo":"","autoReload":false},"effects":[],"folder":null,"sort":0,"permission":{"default":0,"DX8acFfCTTwwzgWq":3},"flags":{"journal-links":{},"core":{"sourceId":"Item.ePXG5XDJxwCsJxn2"},"cf":{"id":"temp_r9d6jaay8q","path":"Savage Eberron Equipment#/CF_SEP/2.1. Meleee Weapons#/CF_SEP/Martial Weapons","color":"#000000"}}},{"_id":"TGHNPmS6nwqGxUGL","name":"Chain Mail Coif or Helm","type":"armor","img":"icons/equipment/head/helm-barbute-engraved.webp","data":{"description":"","notes":"","additionalStats":{},"quantity":1,"weight":4,"price":10,"equippable":false,"equipped":true,"minStr":"d8","armor":3,"isNaturalArmor":false,"locations":{"head":true,"torso":false,"arms":false,"legs":false}},"effects":[],"folder":null,"sort":0,"permission":{"default":0,"DX8acFfCTTwwzgWq":3},"flags":{"journal-links":{},"core":{"sourceId":"Item.nFgT4KITkQxN61yV"},"cf":{"id":"temp_f6vjni9axvd","path":"Savage Eberron Equipment#/CF_SEP/1.1. Armor#/CF_SEP/Medium Armor","color":"#000000"}}},{"_id":"drnnn1WFtlcHCPbe","name":"Chain Mail Hauberk","type":"armor","img":"icons/equipment/chest/breastplate-scale-grey.webp","data":{"description":"","notes":"","additionalStats":{},"quantity":1,"weight":25,"price":75,"equippable":false,"equipped":true,"minStr":"d8","armor":3,"isNaturalArmor":false,"locations":{"head":false,"torso":true,"arms":true,"legs":false}},"effects":[],"folder":null,"sort":0,"permission":{"default":0,"DX8acFfCTTwwzgWq":3},"flags":{"journal-links":{},"core":{"sourceId":"Item.X3b7JulU9PXYrUp6"},"cf":{"id":"temp_f6vjni9axvd","path":"Savage Eberron Equipment#/CF_SEP/1.1. Armor#/CF_SEP/Medium Armor","color":"#000000"}}},{"_id":"tIOzbvbcLlO7MyPr","name":"Chain Mail Leggings","type":"armor","img":"icons/equipment/leg/cuisses-quilted-blue.webp","data":{"description":"","notes":"","additionalStats":{},"quantity":1,"weight":10,"price":30,"equippable":false,"equipped":true,"minStr":"d8","armor":3,"isNaturalArmor":false,"locations":{"head":false,"torso":false,"arms":false,"legs":true}},"effects":[],"folder":null,"sort":0,"permission":{"default":0,"DX8acFfCTTwwzgWq":3},"flags":{"journal-links":{},"core":{"sourceId":"Item.Sgi6uemXI7y4Ra1L"},"cf":{"id":"temp_f6vjni9axvd","path":"Savage Eberron Equipment#/CF_SEP/1.1. Armor#/CF_SEP/Medium Armor","color":"#000000"}}},{"_id":"3e0iNtOz7UychkPd","name":"Heater Shield","type":"shield","img":"icons/equipment/shield/heater-steel-boss-red.webp","data":{"description":"<div class=\"swade-core savage-eberron-core\">\n<p><strong>Hardness:</strong> 10</p>\n<p>While worn a shield can be used to bash for @Compendium[swade-core-rules.swade-rules.Traits]{Strength}+d4 damage.</p>\n</div>","notes":"","additionalStats":{},"quantity":1,"weight":6,"price":10,"equippable":false,"equipped":true,"actions":{"skill":"","skillMod":"","dmgMod":"","additional":{}},"minStr":"d6","parry":2,"cover":2},"effects":[],"folder":null,"sort":0,"permission":{"default":0,"DX8acFfCTTwwzgWq":3},"flags":{"journal-links":{},"core":{"sourceId":"Item.cSV4a1ojLyXfHk9d"},"cf":{"id":"temp_oiuwcer6kfa","path":"Savage Eberron Equipment#/CF_SEP/1.1. Armor#/CF_SEP/Shields","color":"#000000"}}}],"effects":[],"folder":null,"sort":0,"permission":{"default":0,"DX8acFfCTTwwzgWq":3},"flags":{"journal-links":{"references":{}},"cf":{"id":"temp_i6geimupq9","path":"Savage Eberron Bestiary#/CF_SEP/Humanoid#/CF_SEP/House Deneith Guilds","color":